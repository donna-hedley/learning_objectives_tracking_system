<h1>Edit Objective</h1>
<%= form_with scope: @objective, url: objective_path, local: true do |form| %>

<p>
<%= form.label :title %><br>
<%= form.text_field :title %>
</p>

<p>
  <%= form.label :details %><br>
  <%= form.text_area :details %>
</p>
<p>
<%= form.submit %>
</p>
<% end %>


<br><button> Return</button>
